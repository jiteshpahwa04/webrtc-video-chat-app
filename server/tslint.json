{
    "extends": "tslint:recommended",
    "defaultSeverity": "error",
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "max-line-length": [true, 120],
        "semicolon": {
            "options": [
                "always"
            ]
        }
    },
    "jsonRules": {
        "check-keys": true,
        "no-trailing-comma": true
    }
}